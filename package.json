{
  "name": "proshop2",
  "version": "2.0.0",
  "description": "E-commerce application",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "drop:index": "node backend/dropIndex",
    "build": "npm install && cd frontend && npm install && npm run build",
    "generate-toc": "markdown-toc -i readme.md",
    "killports": "node killports.js",
    "clean-start": "node killports.js && npm run dev"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.9",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.0",
    "jspdf": "^3.0.1",
    "mongoose": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.3",
    "pdfkit": "^0.17.1",
    "react-chartjs-2": "^5.3.0",
    "react-datepicker": "^8.3.0",
    "react-image-magnify": "^2.7.4",
    "react-multi-carousel": "^2.8.6",
    "react-quill": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "markdown-toc": "^1.2.0",
    "nodemon": "^3.1.0"
  }
}
